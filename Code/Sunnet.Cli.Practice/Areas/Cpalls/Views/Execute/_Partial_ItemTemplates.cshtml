<script type="text/html" id="_cpalls_Item_12">
    <!--TxkeaReceptive:12-->
    <div class="item-main-body" data-bind="cpalls_item: isShowing(), style: { background: !$root.visible() ? '#BCBCBC' : '#ffffff' }">
        <svg version="1.1" height="0" width="0">
            <filter id="grayscale">
                <feColorMatrix type="matrix" values="0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0" />
            </filter>
        </svg>
        <div id="divContent"   data-bind="style: {position: 'relative', background: itemLayout.background, 'background-image': 'url(' + itemLayout.backgroundImage.src + ')', 'background-size': '100% 100%' }"
            >
            <!-- ko foreach:{data:itemLayout.ImageList.filter(function(obj){return obj.src}),as:'image'}-->
            <!-- ko if:imageType===1-->
           
            <a   data-bind="css: { 'txkeaactive': choosed() }, event: { click: $parent.canChoose() ? $root.choose : '' }, attr: { id: 'img_txkea_' + id }, style: { top: top ? top + '%' : '0px', left: left ? left + '%' : '0px', height: height ? height + '%' : '120px', width: width ? width + '%' : '120px', display: notDelay ? '' : 'none' }"
                 href="#" class="item-pic-con" style="position: absolute">
                <svg style="padding:10px; z-index: 1000;" data-bind="style: { width: '100%', height: '100%' }">
                    <image data-bind="attr:{ 'xlink:href':src, filter: !$parent.canChoose() ? 'url(#grayscale)' : $parent.isIE ? 'url()' : '' , width: '100%', height:  '100%',z_index: 1000 }"
                           xlink:href="" ></image>
                </svg>
                <span class="hide">item</span>
            </a>
            <!--/ko-->
            <!-- ko ifnot:imageType===1-->
            <img data-bind="attr: { id: 'img_txkea_' + id, src: src }, style: {padding:'10px', top: top ? top + '%' : '0px', left: left ? left + '%' : '0px', width: width ? width + '%' : '120px', height: height ? height + '%' : '120px', display: timeout ? 'none' : '' }"
                 style="position: absolute" alt="image" />
            <!--/ko-->
            <!--/ko-->
            <!--ko if:instructionText-->
            <p class="instructionText" data-bind="style: { top: instructionPosition.top + '%', left: instructionPosition.left + '%' }, html: instructionText"></p>
            <!--/ko-->
        </div>
    </div>
    
</script>

<script type="text/html" id="_cpalls_Item_13">
    <!--TxkeaExpressive:13-->
    <div id="divbodyContent" class="item-main-body" data-bind="cpalls_item: isShowing(), style: { background: !$root.visible() ? '#BCBCBC' : '#ffffff' }">

        <!-- ko if:responseType===2-->
        <div id="divContent" data-bind="style: { position: 'relative',background: itemLayout.background, 'background-image': 'url(' + itemLayout.backgroundImage.src + ')', 'background-size': '100% 100%' }, visible: !ShowResponse()">
            <!-- ko foreach:{data:itemLayout.ImageList.filter(function(obj){return obj.src}),as:'image'}-->
            <a href="#" class="item-pic-con" data-bind=" attr: { id: 'img_txkea_' + id }, style: { top: top ? top + '%' : '0px', left: left ? left + '%' : '0px', width: width ? width + '%' : '120px', height: height ? height   + '%' : '120px', display: notDelay ? '' : 'none' }" style="position: absolute">
                <img data-bind="attr: { src: src }, style: { width: '100%', height: '100%' }" alt="image"/>
            </a>
            <!--/ko-->
            <!--ko if:instructionText-->
            <p class="instructionText" data-bind="style: { top: instructionPosition.top + '%', left: instructionPosition.left + '%' }, html: instructionText"></p>
            <!--/ko-->
        </div>
        <div id="divResponseContent"
             data-bind="style: { left:'40px',overflow:'auto', position: 'relative', background: responseBackground.background, 'background-image': 'url(' + responseBackground.backgroundImage + ')', 'background-size': '100% 100%' }, visible: ShowResponse()">
            <br/>
            <!-- ko foreach:{data:responses,as:'response'}-->
            <br/>
            <br/>
            <p data-bind="css: { 'has-error': required && alerting() }">
                <strong><span data-bind="text: Text" class="control-label" style="font-size: 24px;"></span></strong>
            </p>
            <br/>
            <!--ko if:response.Type == 1-->
            <textarea cols="20" role="2" style="width: 400px; height: 100px; border: 1px solid #dddddd; background-color: #ffffff;" data-bind="value: content" title=" "></textarea>
            <!--/ko-->
            <!--ko if:response.Type != 1-->
            <!-- ko foreach:{data:Options,as:'option'}-->
            <p>
                <!--ko if:response.Type == 3-->
                <input type="radio" data-bind="checked: IsCorrect, checkedValue: true, attr: { id: 'radioOption_' + ID, name: 'radioOption_' + response.ID }, click: changeRadio.bind($data, $parent)" title=" "/>&nbsp;&nbsp;&nbsp;
                <label data-bind="text: Lable, attr: { for: 'radioOption_' + ID }"></label>
                <!--/ko-->
                <!--ko if:response.Type == 4-->
                <input type="checkbox" data-bind="checked: IsCorrect, attr: { id: 'chkOption_' + ID }, click: changeRadio.bind($data, $parent)" title=" "/>&nbsp;&nbsp;&nbsp;
                <label data-bind="text: Lable, attr: { for: 'chkOption_' + ID }"></label>
                <!--/ko-->
                <!--ko if:AddTextbox-->
                &nbsp;&nbsp;
                <label style="display: none" data-bind="text: Lable, attr: { for: 'otherText' + ID }"></label>
                <input type="text" style="width: 260px; height: 25px; border: 1px solid #dddddd; background-color: #ffffff;" data-bind="value: otherText, visible: IsCorrect(), attr: { id: 'otherText' + ID }" title=" "/>
                <!--/ko-->
            </p>
            <!--/ko-->
            <!--/ko-->
            <!--/ko-->
        </div>
        <!--ko if:ShowResponse()-->
        <div style="padding-left: 40px;">
            <a class="item-btn next-btn" href="#" data-bind="event: { click: ResetQuestion }, visible: ShowResponse()">
                Question
            </a>
        </div>
        <!--/ko-->
        <!--ko if:!ShowResponse()-->
        <div style="float: right;">
            <a class="item-btn next-btn" href="#" data-bind="event: { click: txkeaSwitch }, visible: !ShowResponse()">
                Response
            </a>
        </div>
        <!--/ko-->
        <!--/ko-->
        @*    responseType===2 end*@

        <!-- ko if:responseType===1-->
        <div  id="divContent" style="position: relative"  data-bind="style: { background: itemLayout.background, 'background-image': 'url(' + itemLayout.backgroundImage.src + ')', 'background-size': '100% 100%' }, visible: !ShowResponse()"
           >
            <!-- ko foreach:{data:itemLayout.ImageList,as:'image'}-->
            <a href="#" class="item-pic-con" data-bind=" attr: { id: 'img_txkea_' + id }, style: { top: top ? top + '%' : '0px', left: left ? left + '%' : '0px', width: width ? width  + '%' : '120px', height: height ? height + '%' : '120px', display: notDelay ? '' : 'none' }" style="position: absolute">
                <img data-bind="attr: { src: src }, style: { width:  '100%', height:  '100%' }" alt="image"/>
            </a>
            <!--/ko-->
            <!--ko if:instructionText-->
            <p class="instructionText" data-bind="style: { top: instructionPosition.top + '%', left: instructionPosition.left + '%' }, html: instructionText"></p>
            <!--/ko-->
        </div>
        <!--/ko-->
        @*    responseType===1 end*@
    </div>
</script>

