<script type="text/html" id="_cpalls_Item_11_Response">
    <div class=" item-response well">
        <!--ko if:picture-->
        <div class="item-response-img thumbnail">
            <a href="#" class="item-pic-con" data-bind="html: html, visible: pictureTimeIn === 0, showIn: pictureTimeIn"></a>
            <a href='#' class='time' data-bind="html: execHelper.waitingHtml, visible: pictureTimeIn > 0, hideIn: pictureTimeIn"></a>
        </div>
        <!--/ko-->

        <div class="item-response-input" data-bind="css: { 'has-error': required && alerting() }" style="height: 40px">
            <!-- ko if:type === TypedResponseType.Text-->
            <input class="form-control" type="text"
                   data-bind="attr: { id: uuid, maxlength: length, placeholder: 'Max Characters: ' + length }, value: content" style="font-size: 20px; height: 30px" />
            <!--/ko-->
            <!-- ko if:type === TypedResponseType.Numerical-->
            <input class="_number form-control"
                   data-bind="attr: { id: uuid, float: length}, value: content"
                   min="0" max="10000" style="font-size: 20px; height: 30px" />
            <!--/ko-->
        </div>

        <div class="item-response-label" style="font-size: 20px;">
            <label data-bind="attr: { 'for': uuid }, html: text, visible: textTimeIn === 0, showIn: textTimeIn"></label>
        </div>
    </div>
</script>
