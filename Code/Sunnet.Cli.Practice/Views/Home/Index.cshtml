@using Sunnet.Cli.Business.Cpalls.Models;
@{
    ViewBag.Title = "Assessment Practice Area";
}
@section LeftMenu
{
    @Html.Partial("_Partial_Left_Cpalls")
}
@section BreadPath
{
    Assessment<i class="icon-angle-right"></i>Assessment Practice Area
}
@section Scripts
{
    <script type="text/javascript">
        $(function () {
            var message = "@Html.Raw(ViewBag.Message)";
            if (message) {
                window.showMessage("fail", message);
            }
        });
    </script>
}
<div class="right-content">
    <ul class="assessment-menu-dashboard">

        <div class="assessment-d-btns">
            @if (ViewBag.ShowCPALLS)
            {
                foreach (CpallsAssessmentModel item in @ViewBag.List as List<CpallsAssessmentModel>)
                {
                    <li>
                        @Html.ActionLink(@item.Name, "Index", "Student",
                            new { Area = "Cpalls", assessmentId = @item.ID },
                            new { @class = "cpalls-btn" })
                    </li>
                }
            }
        </div>

    </ul>
</div>


