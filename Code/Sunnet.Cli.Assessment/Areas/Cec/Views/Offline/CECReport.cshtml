@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CEC - Report</title>
    <style type="text/css">
        body, h1, h2, h3, h4, h5, h6, p, ul, ol, li, form, img, dl, dt, dd, table, th, td, blockquote, fieldset, div, strong, label, em {
            margin: 0;
            padding: 0;
            border: 0;
        }

        body {
            font-family: Calibri;
            font-size: 12px;
        }

        table > caption {
            font-size: 14px;
            font-weight: bold;
        }

        .container {
            /*border: solid 1px #000;*/
            border-radius: 5px;
            padding: 5px 20px;
            margin: 10px;
            text-align: center;
            width: 95%;
        }

            .container.pdf {
                margin: 0px;
                padding: 0px;
            }

                .container.pdf table {
                }

        table {
            margin-bottom: 20px;
        }

            table.header {
                width: 99%;
                margin: 0 auto;
            }

        .header div {
            white-space: nowrap;
        }

        .header td {
            vertical-align: top;
            padding: 0px;
        }

        table th, table td {
            padding: 3px;
        }

            table th.noborder, table td.noborder, .noborder, .noborder td, .noborder th {
                border-color: #fff !important;
            }

            table th.border-t, table td.border-t {
                border-top: 1px solid #000 !important;
            }

            table th.border-l, table td.border-l {
                border-left: 1px solid #000 !important;
            }

        table caption {
            text-align: left;
        }
        /***************** begin:table-left ********************/
        .table-left {
            text-align: left;
            border: #fff !important;
        }

            .table-left .noborder {
                border-color: #fff !important;
            }

            .table-left th, .table-left td {
                border: solid 1px #000;
                padding: 3px;
            }

            .table-left .td-right {
                text-align: right;
            }

            .table-left .td-center {
                text-align: center;
            }

            .table-left .text-bold {
                font-weight: bolder;
            }

        /***************** end:table-left ********************/

        /***** fixed columns start**********/
        .fixed-columns-container {
            position: relative;
        }

        .fixed-columns-overflow {
            margin-left: 99px;
            overflow-x: scroll;
            overflow-y: visible;
            padding-bottom: 1px;
        }

        table > thead > tr.fixed-columns-rotate th, .table > thead > tr > th.fixed-columns-rotate-super {
            border: none;
            padding: 0px;
            position: relative;
            vertical-align: bottom;
            min-width: 50px;
        }

        table > thead > tr.fixed-columns-rotate > th.fixed-columns-rotate-super {
            height: 30px;
        }

        table > thead > tr.fixed-columns-rotate th.fixed-columns-rotate {
            max-width: 70px;
            height: 130px;
        }

        th.fixed-columns-rotate {
            border: none;
        }


            th.fixed-columns-rotate > div {
                white-space: normal;
                height: 70px;
                left: 72px;
                z-index: 100;
                position: relative;
                min-width: 10px;
                width: 130px;
                overflow: hidden;
                border: 1px solid #000;
                -webkit-transform: rotate(270deg) skew(0deg,30deg); /* Safari and Chrome */
                -webkit-transform-origin: bottom left; /* Safari and Chrome */
                transform: rotate(270deg) skew(0deg,30deg);
                transform-origin: bottom left;
                -ms-transform: rotate(270deg) skew(0deg,30deg);
                -ms-transform-origin: bottom left;
                -moz-transform: rotate(270deg) skew(0deg,30deg);
                -moz-transform-origin: bottom left;
                -o-transform: rotate(270deg) skew(0deg,30deg);
                -o-transform-origin: bottom left;
            }

        th.fixed-columns-rotate-super > div {
            white-space: normal;
            height: 30px;
            z-index: 100;
            position: relative;
            left: 75px;
            min-width: 10px;
            width: 100%;
            overflow: hidden;
            border: 1px solid #000;
            -webkit-transform: skew(-30deg,0deg); /* Safari and Chrome */
            -webkit-transform-origin: bottom left; /* Safari and Chrome */
            transform: skew(-30deg,0deg);
            transform-origin: bottom left;
            -ms-transform: skew(-30deg,0deg);
            -ms-transform-origin: bottom left;
            -moz-transform: skew(-30deg,0deg);
            -moz-transform-origin: bottom left;
            -o-transform: skew(-30deg,0deg);
            -o-transform-origin: bottom left;
        }

        table thead .active.fixed-columns-rotate, table thead .active.fixed-columns-rotate-super {
            background: none !important;
        }

            table thead .active.fixed-columns-rotate > div, table thead .active.fixed-columns-rotate-super > div {
                background: #f5f5f5;
            }

        th.fixed-columns-rotate > div > span {
            position: absolute;
            left: 10px;
            bottom: 0px;
            text-align: left;
            display: inline-block;
            width: 97%;
            height: 40px;
            -webkit-transform: skew(0deg, -30deg) rotate(30deg); /* Safari and Chrome */
            transform: skew(0deg, -30deg) rotate(30deg);
            -ms-transform: skew(0deg, -30deg) rotate(30deg);
            -moz-transform: skew(0deg, -30deg) rotate(30deg);
            -o-transform: skew(0deg, -30deg) rotate(30deg);
        }

        th.fixed-columns-rotate-super > div > span {
            position: absolute;
            left: 0px;
            bottom: 4px;
            display: inline-block;
            width: 100%;
            transform: skew(30deg, 0deg);
            -webkit-transform: skew(30deg, 0deg); /* Safari and Chrome */
            -ms-transform: skew(30deg, 0deg);
            -moz-transform: skew(30deg, 0deg);
            -o-transform: skew(30deg, 0deg);
        }

        .fixed-column, .fixed-column-header {
            position: absolute !important;
            left: 0px;
            width: 100px;
            border: 1px solid #000;
        }

            .fixed-column-header div {
                margin-top: 94px;
            }

        .fixed-columns-container thead > th.fixed-column {
            border-top: 1px;
        }

        .cpalls_four, .cpalls_three {
            color: #fff;
            font-weight: bold;
        }

            .cpalls_four span, .cpalls_no_benchmark span, .cpalls_three span, .cpalls_normal span {
                display: inline-block;
                border-radius: 3px;
                width: auto;
                min-width: 30px;
                padding: 2px 5px;
            }

            .cpalls_four span {
                background: #dc143c;
                color: white;
            }

        .cpalls_no_benchmark span {
            background-color: #028dc4;
            color: white;
        }

        .cpalls_three span {
            background: #e8a645;
            color: white;
        }

        .cpalls_normal span {
            background: #247432;
            color: white;
        }

        .newpager {
            page-break-before: always;
        }

        .btn-export {
            position: fixed;
            top: 50%;
            right: 50px;
            height: 40px;
            font-size: 16px;
            color: #fff;
            cursor: pointer;
            display: none;
        }

        .title-strong {
            font-weight: bold;
        }

        .note {
            text-align: left;
            color: #999;
        }

        .report-data {
            margin: 0 auto;
        }

        img {
            max-width: 1200px;
            max-height: 650px;
        }
        /***** fixed columns end**********/

        .table_border {
            border: 1px #333 solid;
            border-left: 0px;
            border-bottom: 0px;
        }

            .table_border td {
                border-left: 1px #333 solid;
                border-bottom: 1px #333 solid;
            }

             table {
                border: none;
                margin: 0;
                padding: 0;
                font-family: Calibri;
                font-size: 12px;
            }

                table td, table th {
                    border: none;
                    margin: 0;
                    padding: 0;
                }

   

                table td {
                }

                    table td table td {
                        border-bottom: 2px solid #7caab5;
                        padding: 10px 10px;
                        border-right: 1px solid #7caab5;
                    }

                    table td table th {
                        border-top: 1px solid #7caab5;
                        border-right: 1px solid #7caab5;
                    }
    </style>
</head>
    <body>
        <div class='container '>
            <div class="main-content main-content">
                <div class="content-body-tab">
                    <table class="table table-striped table-hover" width="99%" cellspacing="0" cellpadding="0" border="0" style="margin-bottom: 20px;">
                        <thead>
                            <tr>
                                <td style="border-bottom: 0px; background-color: #fff;">
                                    <table class="table table-striped table-hover" width="100%">
                                        <tr>
                                            <td style="width: 180px; border-bottom: 0px; background-color: #fff; border: 0px">&nbsp;</td>
                                            <td style="font-size: 14px; font-weight: bold; border-bottom: 0px; background-color: #fff; border: 0px" data-bind="html: reportTitle"></td>
                                            <td style="width: 180px; border-bottom: 0px; background-color: #fff; border: 0px">
                                                <img src="/images/cli_logo.png" style="width: 150px; height: 60px; vertical-align: middle;" alt="cli_logo" /></td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td style="border-bottom: 0px; background-color: #fff; border: 0px;">
                                    <table class="table table-striped table-hover" width="100%" cellspacing="0" cellpadding="0" border="0" data-bind="foreach: cecitems">
                                        <tr style="border-top: 1px solid #7caab5">
                                            <td colspan="3" style="text-align: left; font-size: 14px; font-weight: bold; border-bottom: 0px; padding-top: 20px; padding-bottom: 10px; background-color: #fff; border: 0px;" data-bind="text: MeasureName"></td>
                                            <td style="border-bottom: 0px; background-color: #fff; border-right: 0px;" data-bind="foreach: Links">
                                                <p><a target="_blank" data-bind="text: DisplayText, attr: { href: Link }"></a></p>
                                            </td>
                                        </tr>
                                        <!-- ko if:  Childer != null -->
                                        <!-- ko foreach: Childer -->
                                        <tr>
                                            <td colspan="3" style="text-align: left; font-size: 13px; font-weight: bold; padding-left: 20px; padding-top: 10px; padding-bottom: 10px; border: 0px; background-color: #fff;" data-bind="text: MeasureName"></td>
                                            <td style="background-color: #fff; border: 0px;" data-bind="foreach: Links">
                                                <p><a target="_blank" data-bind="text: DisplayText, attr: { href: Link }"></a></p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <th style="width: 70px; border-bottom: 1px solid #7caab5; border-left: 1px solid #7caab5;">CEC Value</th>
                                            <th style="width: 20%; border-bottom: 1px solid #7caab5;">Area</th>
                                            <th style="border-bottom: 1px solid #7caab5; padding: 10px 0px;">Description</th>
                                            <th style="border-bottom: 1px solid #7caab5; padding: 10px 0px; width: 10%">Photograph Links</th>
                                        </tr>

                                        <!-- ko foreach: List -->
                                        <tr>
                                            <td style="border-left: 1px solid #7caab5;" data-bind="text: Score==0?'':Score"></td>
                                            <td style="text-align: left; padding-left: 5px;" data-bind="text: Area"></td>
                                            <td style="text-align: left;">
                                                <div data-bind="html: Description"></div>
                                            </td>
                                            <td data-bind="foreach: Links">
                                                <p><a target="_blank" data-bind="text: DisplayText, attr: { href: Link }"></a></p>
                                            </td>
                                        </tr>
                                        <!-- /ko -->
                                        <!-- /ko -->
                                        <!-- /ko -->
                                        <!-- ko if:  Childer == null -->
                                        <tr>
                                            <th style="width: 70px; border-bottom: 1px solid #7caab5; border-left: 1px solid #7caab5;">CEC Value</th>
                                            <th style="width: 20%; border-bottom: 1px solid #7caab5;">Area</th>
                                            <th style="border-bottom: 1px solid #7caab5; padding: 10px 0px;">Description</th>
                                            <th style="border-bottom: 1px solid #7caab5; padding: 10px 0px; width: 10%">Photograph Links</th>
                                        </tr>
                                        <!-- ko foreach: List -->
                                        <tr>
                                            <td style="border-left: 1px solid #7caab5;" data-bind="text: Score==0?'':Score"></td>
                                            <td style="text-align: left; padding-left: 5px;" data-bind="text: Area"></td>
                                            <td style="text-align: left;">
                                                <div data-bind="html: Description"></div>
                                            </td>
                                            <td data-bind="foreach: Links">
                                                <p><a target="_blank" data-bind="text: DisplayText, attr: { href: Link }"></a></p>
                                            </td>
                                        </tr>
                                        <!-- /ko -->
                                        <!-- /ko -->
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td height="52px">
                                    <a style="background:#ca6306; padding:8px 20px; color:#fff; text-decoration:none; font-size:14px;" type="button" href="@Url.Action("Index")">Back</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        @System.Web.Optimization.Scripts.Render("~/scripts/jquery/offline")
        @System.Web.Optimization.Scripts.Render("~/scripts/bootstrap/offline")
        @System.Web.Optimization.Scripts.Render("~/scripts/knockout/offline")
        @System.Web.Optimization.Scripts.Render("~/scripts/cec/offline")

        <script type="text/javascript">
            var offlineApp;
            $(function () {

                offlineApp = getCecOfflineApp("@(ViewBag.EncryptUserID)");
                offlineApp.init();

                ko.applyBindings(offlineApp.viewModel);

            });
        </script>
    </body>
</html>
