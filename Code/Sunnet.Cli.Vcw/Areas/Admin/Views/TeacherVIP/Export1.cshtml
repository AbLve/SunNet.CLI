<style type="text/css">
    td {
        position: relative;
        text-align: center;
    }

    .color-icon-w {
        left: 50%;
        position: absolute;
        display: inline-block;
    }

    .color-icon {
        right: 10px;
        position: relative;
        font-size: 8px;
        line-height: 20px;
        display: inline-block;
    }

    .color-icon-b {
        font-size: 12px;
        line-height: 18px;
    }

    .color-icon-r {
        right: 10px;
        font-size: 9px;
        line-height: 15px;
    }

    .map-description {
        /*position: absolute;
        right: 100px;
        bottom: 50px;*/
        float: right;
        border: 1px #333 solid;
        padding: 0px 10px;
        width: 202px;
    }

        .map-description p {
            height: 26px;
            line-height: 26px;
            width: 90px;
            float: left;
        }

            .map-description p span {
                display: inline-block;
                width: 15px;
            }
</style>

