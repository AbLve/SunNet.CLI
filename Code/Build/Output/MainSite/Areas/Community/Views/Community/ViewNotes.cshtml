@using System.Web.Optimization
@using System.Web.UI.WebControls
@using Sunnet.Cli.UIBase
@using Sunnet.Cli.UIBase.Models
@model Sunnet.Cli.Core.Communities.Entities.CommunityNotesEntity

@{
    ViewBag.Title = ViewBag.CommunityName;
    Layout = "~/Views/Shared/_Modal_Layout.cshtml";
}


<div class="form-group" style="height: 400px; overflow-y: auto">
    <div class="col-sm-12">
        @if (!string.IsNullOrEmpty(ViewBag.LogoUrl) && Model.DisplayLogo)
        {
            <div style="height: 120px; text-align: center">
                <img alt="@ViewBag.CommunityName" src="@FileHelper.GetPreviewPathofUploadFile(ViewBag.LogoUrl)" width="250px" height="100px" />
            </div>
        }
        @Html.Raw(@Model.Messages)
    </div>
</div>
