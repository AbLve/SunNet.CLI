<script type="text/html" id="_cpalls_Item_3">
    <!--MultipleChoices:3-->
    <div class="item-main-body" data-bind="cpalls_item:isShowing()">
        <div class="item-text-con">
            <p class="target">
            </p>
        </div>
        <div class="item-body-con item-body-con-manypic">
            <ul class="answer" data-bind="foreach:{data:answers,as:'answer'}">
                <li data-bind="css:{'active':choosed()}">
                    <!--ko if:picture.file-->
                    <a href="#" class="item-pic-con" data-bind="event:{click:$root.choose},html:picture.html,visible:picture.timeout==0,showIn:picture.timeout">
                    </a>
                    <a href='#' class='time' data-bind="html:execHelper.waitingHtml,visible:picture.timeout>0,hideIn:picture.timeout"></a>
                    <!--/ko-->
                </li>
                <!--ko if: $index() == 3-->
                <li class="blank" style="width:10%">
                </li>
                <!--/ko-->
            </ul>
        </div>
    </div>
</script>
