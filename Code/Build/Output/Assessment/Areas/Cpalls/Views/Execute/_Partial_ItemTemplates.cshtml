<script type="text/html" id="_cpalls_Item_11_Response">
    <div class=" item-response well">
        <div class="item-response-img thumbnail">
            <!--ko if:picture-->
            <a href="#" class="item-pic-con" data-bind="html:html,visible:pictureTimeIn===0,showIn:pictureTimeIn">
            </a>
            <a href='#' class='time' data-bind="html:execHelper.waitingHtml,visible:pictureTimeIn>0,hideIn:pictureTimeIn"></a>
            <!--/ko-->
        </div>
        <div class="item-response-label">
            <label data-bind="attr:{'for':uuid},html:text,visible:textTimeIn===0,showIn:textTimeIn"></label>
        </div>
        <div class="item-response-input" data-bind="css:{'has-error': required && alerting()}">
            <!-- ko if:type === TypedResponseType.Text-->
            <input class="form-control" type="text"
                   data-bind="attr:{id:uuid,maxlength:length,placeholder:'Max Characters: '+length},value:content" />
            <!--/ko-->
            <!-- ko if:type === TypedResponseType.Numerical-->
            <input class="form-control" type="number"
                   data-bind="attr:{id:uuid,maxlength:length,placeholder:'Number of decimals: '+length},value:content"
                   min="0" max="10000" />
            <!--/ko-->
        </div>
    </div>
</script>
