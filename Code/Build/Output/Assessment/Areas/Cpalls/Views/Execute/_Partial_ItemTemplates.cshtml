<script type="text/html" id="_cpalls_Item_10_1">
    <!--Checklist:10,for Checklist Measure-->
    <tr data-bind="if:item.type == Ade_ItemType.Checklist.value">
        <td data-bind="html:target.text"></td>
        <td>
            <!--ko foreach:{data:answers,as:'answer'}-->
            <!--ko if:item.multiChoice-->
            <a class="item-list-btn " data-bind="event:{click:function(){$root.choose(answer,item);}},css:{'item-list-btn-tr':item.direction==Cpalls_Direction.Landscape,'item-list-btn-td':item.direction==Cpalls_Direction.Portrait}">
                <i data-bind="css:{'icon-check':choosed(),'icon-check-empty':!choosed()}"></i>
                <span data-bind="html:($index()+1)+ ' - ' +text.text"></span>
            </a>
            <!-- /ko-->
            <!--ko ifnot:item.multiChoice-->
            <a class="item-list-btn" data-bind="event:{click:function(){$root.choose(answer,item);}},css:{'item-list-btn-tr':item.direction==Cpalls_Direction.Landscape,'item-list-btn-td':item.direction==Cpalls_Direction.Portrait}">
                <i data-bind="css:{'icon-circle':choosed(),'icon-circle-blank':!choosed()}"></i>
                <span data-bind="html:($index()+1)+ ' - ' +text.text"></span>
            </a>
            <!-- /ko-->
            <!-- /ko-->

        </td>
    </tr>
</script>
